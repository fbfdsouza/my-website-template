{
  "name": "crawling-app-challenge",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^1.2.3",
    "concurrently": "^7.6.0",
    "history": "^5.3.0",
    "json-server": "^0.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.7.0",
    "styled-components": "^5.3.6"
  },
  "scripts": {
    "start": "concurrently \"yarn run dev:server\" \"yarn run json-server\"",
    "dev:server": "webpack-dev-server --watch --config config/webpack.dev --mode=development --open",
    "prod:server": "node server.js --config build/webpack.prod --mode=production",
    "prod:build": "webpack --config ./config/webpack.prod --mode=production",
    "dev": "webpack --watch --config ./config/webpack.dev --mode=development",
    "prod": "webpack --config ./config/webpack.prod --mode=production",
    "build": "yarn run prod:build",
    "lint": "eslint --cache --ext .js --ext .jsx",
    "lint:fix": "eslint --ext .js --ext .jsx --fix",
    "test": "jest",
    "json-server": "json-server --watch db.json --port 3001"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.6",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "html-loader": "^1.2.1",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.0.5",
    "style-loader": "^2.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.0.0-rc.3",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.2"
  }
}
